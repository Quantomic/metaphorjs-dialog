{
    "version": "1.0.4",
    "push": [
        "github"
    ],
    "build": {
        "standalone": {
            "wrap": true,
            "global": true,
            "target": "dist/metaphorjs.dialog.js",
            "files": [
                [
                    "../metaphorjs/src/MetaphorJs.js",
                    {
                        "as": "MetaphorJs"
                    }
                ],
                [
                    "src/metaphorjs.dialog.js",
                    {
                        "as": "Dialog"
                    }
                ],
                [
                    "../metaphorjs/src/func/dom/onReady.js",
                    {
                        "as": "onReady"
                    }
                ],
                "src/jquery/plugin.js"
            ],
            "expose": [
                [
                    "MetaphorJs.lib",
                    "Dialog"
                ],
                "onReady"
            ]
        },
        "npm": {
            "target": "dist/metaphorjs.dialog.npm.js",
            "compile": false,
            "files": [
                [
                    "src/metaphorjs.dialog.js",
                    {
                        "keepExports": true
                    }
                ]
            ],
            "omit": [
                "../metaphorjs-observable/src/metaphorjs.observable.js",
                "../metaphorjs-promise/src/metaphorjs.promise.js",
                "../metaphorjs-ajax/src/metaphorjs.ajax.js",
                "../metaphorjs-select/src/metaphorjs.select.js",
                "../metaphorjs-animate/src/metaphorjs.animate.js",
                "../metaphorjs-animate/src/func/stopAnimation.js"
            ],
            "require": {
                "metaphorjs-observable": "Observable",
                "metaphorjs-promise": "Promise",
                "metaphorjs-ajax": "ajax",
                "metaphorjs-select": "select",
                "metaphorjs-animate": "animate"
            }
        },
        "amd": {
            "target": "dist/metaphorjs.dialog.amd.js",
            "files": [
                [
                    "src/metaphorjs.dialog.js",
                    {
                        "as": "Dialog"
                    }
                ],
                "src/jquery/plugin.js"
            ],
            "omit": [
                "../metaphorjs-observable/src/metaphorjs.observable.js",
                "../metaphorjs-promise/src/metaphorjs.promise.js",
                "../metaphorjs-ajax/src/metaphorjs.ajax.js",
                "../metaphorjs-select/src/metaphorjs.select.js",
                "../metaphorjs-animate/src/metaphorjs.animate.js",
                "../metaphorjs-animate/src/func/stopAnimation.js"
            ],
            "prepend": [
                "src/amd/prepend.js"
            ],
            "define": {
                "name": "metaphorjs-dialog",
                "return": "Dialog",
                "deps": {
                    "metaphorjs-observable": "Observable",
                    "metaphorjs-promise": "Promise",
                    "metaphorjs-ajax": "ajax",
                    "metaphorjs-select": "select",
                    "metaphorjs-animate": "animate"
                }
            }
        }
    }
}